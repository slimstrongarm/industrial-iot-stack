    "alarm_management": {
      "name": "Industrial Alarm Management",
      "description": "Comprehensive alarm monitoring with prioritization, acknowledgment, and trending",
      "category": "safety",
      "flows": [
        {
          "id": "alarms_tab",
          "type": "ui_tab",
          "name": "Alarm Management",
          "icon": "warning",
          "order": 2,
          "disabled": false,
          "hidden": false
        },
        {
          "id": "active_alarms_group",
          "type": "ui_group",
          "name": "Active Alarms",
          "tab": "alarms_tab",
          "order": 1,
          "disp": true,
          "width": "24",
          "collapse": false
        },
        {
          "id": "alarm_summary",
          "type": "ui_template",
          "group": "active_alarms_group",
          "name": "Alarm Summary",
          "order": 1,
          "width": "24",
          "height": "3",
          "format": "<div style=\"display: flex; justify-content: space-around; padding: 10px; background: linear-gradient(135deg, #667eea 0%, #764ba2 100%); border-radius: 8px; color: white;\"><div style=\"text-align: center;\"><div style=\"font-size: 28px; font-weight: bold; color: #ff4444;\">{{msg.payload.critical || 0}}</div><div>Critical</div></div><div style=\"text-align: center;\"><div style=\"font-size: 28px; font-weight: bold; color: #ffaa00;\">{{msg.payload.warning || 0}}</div><div>Warning</div></div><div style=\"text-align: center;\"><div style=\"font-size: 28px; font-weight: bold; color: #4488ff;\">{{msg.payload.info || 0}}</div><div>Info</div></div><div style=\"text-align: center;\"><div style=\"font-size: 28px; font-weight: bold; color: #44ff44;\">{{msg.payload.acknowledged || 0}}</div><div>Acknowledged</div></div></div>",
          "storeOutMessages": true,
          "fwdInMessages": true,
          "resendOnRefresh": true,
          "templateScope": "local",
          "x": 400,
          "y": 100,
          "wires": [[]]
        },
        {
          "id": "alarm_table",
          "type": "ui_table",
          "group": "active_alarms_group",
          "name": "Active Alarm List",
          "order": 2,
          "width": "24",
          "height": "12",
          "columns": [
            {"field": "timestamp", "title": "Time", "width": "140", "align": "left"},
            {"field": "severity", "title": "Severity", "width": "80", "align": "center"},
            {"field": "equipment", "title": "Equipment", "width": "150", "align": "left"},
            {"field": "tag", "title": "Tag", "width": "120", "align": "left"},
            {"field": "description", "title": "Description", "width": "300", "align": "left"},
            {"field": "value", "title": "Current Value", "width": "100", "align": "right"},
            {"field": "setpoint", "title": "Setpoint", "width": "100", "align": "right"},
            {"field": "ack", "title": "Ack", "width": "60", "align": "center"}
          ],
          "outputs": 1,
          "cts": true,
          "x": 400,
          "y": 200,
          "wires": [["alarm_acknowledge"]]
        },
        {
          "id": "alarm_acknowledge",
          "type": "function",
          "name": "Acknowledge Alarm",
          "func": "// Handle alarm acknowledgment\nif (msg.payload && msg.payload.rowIndex !== undefined) {\n    const alarmData = msg.payload;\n    \n    // Create acknowledgment message\n    const ackMsg = {\n        topic: 'alarm/acknowledge',\n        payload: {\n            alarmId: alarmData.id,\n            acknowledgedBy: global.get('currentUser') || 'System',\n            acknowledgedAt: new Date().toISOString(),\n            rowIndex: alarmData.rowIndex\n        }\n    };\n    \n    node.status({fill: 'green', shape: 'dot', text: `Alarm ${alarmData.id} acknowledged`});\n    \n    return ackMsg;\n}\n\nreturn null;",
          "outputs": 1,
          "noerr": 0,
          "initialize": "",
          "finalize": "",
          "libs": [],
          "x": 600,
          "y": 200,
          "wires": [["alarm_storage"]]
        },
        {
          "id": "alarm_history_group",
          "type": "ui_group",
          "name": "Alarm History & Trends",
          "tab": "alarms_tab",
          "order": 2,
          "disp": true,
          "width": "24",
          "collapse": false
        },
        {
          "id": "alarm_trend_chart",
          "type": "ui_chart",
          "group": "alarm_history_group",
          "name": "Alarm Frequency Trend",
          "order": 1,
          "width": "12",
          "height": "8",
          "label": "Alarms per Hour",
          "chartType": "bar",
          "legend": "true",
          "xformat": "HH:mm",
          "interpolate": "linear",
          "nodata": "No Alarm Data",
          "dot": false,
          "ymin": "0",
          "ymax": "",
          "removeOlder": 1,
          "removeOlderPoints": "",
          "removeOlderUnit": "86400",
          "cutout": 0,
          "useOneColor": false,
          "useUTC": false,
          "colors": ["#d62728", "#ff7f0e", "#2ca02c"],
          "outputs": 1,
          "x": 300,
          "y": 350,
          "wires": [[]]
        },
        {
          "id": "top_alarms_chart",
          "type": "ui_chart",
          "group": "alarm_history_group",
          "name": "Top Alarm Sources",
          "order": 2,
          "width": "12",
